<struts>
	<constant name="struts.devMode" value="false" />
	<constant name="struts.action.extension" value="html" />
	<constant name="struts.custom.i18n.resources" value="ApplicationResources" />
	<package name="qldiem" extends="struts-default" namespace="/">
		<!-- Add your actions here -->
		<action name="index" method="getIndex" class="actions.Home">
			<result name="login" type="freemarker">/WEB-INF/freemarker/login.jsp
			</result>
			<result name="welcome" type="freemarker">/WEB-INF/freemarker/direct.jsp
			</result>
			<result name="directing_page" type="freemarker">${pageDirect}
			</result>
		</action>
		<action name="login" method="authenticate" class="actions.LoginAction">
			<result type="freemarker" name="success">/WEB-INF/freemarker/direct.jsp
			</result>
			<result type="freemarker" name="directing_page">${pageDirect}
			</result>
			<result type="freemarker" name="error">/WEB-INF/freemarker/login.jsp
			</result>
			<result type="freemarker" name="input">/WEB-INF/freemarker/login.jsp
			</result>
		</action>
		<action name="logout" method="logout" class="actions.LogoutAction">
			<result name="success" type="freemarker">/WEB-INF/freemarker/login.jsp
			</result>
		</action>
	</package>
</struts>