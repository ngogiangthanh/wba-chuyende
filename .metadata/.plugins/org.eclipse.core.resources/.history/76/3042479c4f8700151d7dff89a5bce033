package models;

public class hk_nk implements Comparable<hk_nk>{
	private String nk;
	private int hk;
	
	public hk_nk(String nk, int hk) {
		this.nk = nk;
		this.hk = hk;
	}
	
	public void setHk(int hk) {
		this.hk = hk;
	}
	
	public void setNk(String nk) {
		this.nk = nk;
	}
	
	public int getHk() {
		return hk;
	}
	
	public String getNk() {
		return nk;
	}

	@Override
	public int compareTo(hk_nk o) {
		 return this.compareTo(o);
	}
	
    @Override
    public boolean equals(Object object) {
        boolean same = false;

        if (object != null && object instanceof hk_nk) {
            same = (this.nk == null ? ((hk_nk) object).term == null : this.term.equals(((VectorOfDocument) object).term));
        }
        return same;
    }

    @Override
    public int hashCode() {
        int hash = 7;
        hash = 29 * hash + Objects.hashCode(this.term);
        return hash;
    }
}
