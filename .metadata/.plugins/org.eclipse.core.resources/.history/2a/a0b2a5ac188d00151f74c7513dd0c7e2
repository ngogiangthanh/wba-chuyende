package models;

import java.util.Objects;

public class ke_hoach implements Comparable<ke_hoach> {
	private String nk;
	private int hk;
	private float tbctl;
	private int tstctl;

	public ke_hoach(String nk, int hk) {
		this.nk = nk;
		this.hk = hk;
	}

	public void setHk(int hk) {
		this.hk = hk;
	}

	public void setNk(String nk) {
		this.nk = nk;
	}

	public int getHk() {
		return hk;
	}

	public String getNk() {
		return nk;
	}

	@Override
	public int compareTo(ke_hoach o) {
		return this.nk.compareTo(o.getNk()) + this.hk - o.getHk();
	}

	@Override
	public boolean equals(Object object) {
		boolean same = false;

		if (object != null && object instanceof ke_hoach) {
			same = ((ke_hoach) object).nk.equals(this.getNk()) & ((ke_hoach) object).hk == this.getHk();
		}
		return same;
	}

	@Override
	public int hashCode() {
		int hash = 7;
		hash = 29 * hash + Objects.hashCode(this.nk) + Objects.hashCode(this.hk);
		return hash;
	}
	
	public float getTbctl() {
		return tbctl;
	}
	
	public int getTstctl() {
		return tstctl;
	}
	
	public void setTbctl(float tbctl) {
		this.tbctl = tbctl;
	}
	
	public void setTstctl(int tstctl) {
		this.tstctl = tstctl;
	}
}
